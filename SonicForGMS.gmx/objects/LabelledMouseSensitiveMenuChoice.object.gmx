    var width = string_width(label);
    var height = string_height(label);
    
    switch (halign) {
    case fa_left:
        left = floor(x);
        right = ceil(x + width);
        break;
    
    case fa_center:
        left = floor(x - floor(width));
        right = ceil(x + floor(width));
        break;
    
    case fa_right:
        left = floor(x - width);
        right = ceil(x);
        break;
    }
    
    switch (valign) {
    case fa_top:
        top = floor(y);
        bottom = ceil(y + height);
        break;
    
    case fa_middle:
        top = floor(y - floor(height));
        bottom = ceil(y + floor(height));
        break;
    
    case fa_bottom:
        top = floor(y - height);
        bottom = ceil(y);
        break;
    }

    setup = true;
}
</string>

          <argument>
            <kind>1</kind>
            <string>/// Reframe
view_enabled = true;

for (vind = 0; vind &lt; max_view_count; ++vind) {
    view_visible[vind] = false;
    view_xport[vind] = 0;
    view_yport[vind] = 0;
    view_wport[vind] = 0;
    view_hport[vind] = 0;
}

switch (ports) {
case 2: // horizontal split in half
    view_yport[1] = height * 0.5;
    for (vind = 0; vind &lt; 2; ++vind) {
        view_visible[vind] = true;
        view_wport[vind] = width;
        view_hport[vind] = height * 0.5;
    }
    break;

case 3: // horizontal split, then vertical split the bottom half
    view_visible[0] = true;
    view_wport[0] = width;
    view_hport[0] = height * 0.5;
    for (vind = 1; vind &lt; 3; ++vind) {
        view_visible[vind] = true;
        view_xport[vind] = (vind - 1) * width * 0.5;
        view_yport[vind] = height * 0.5;
        view_wport[vind] = width * 0.5;
        view_hport[vind] = height * 0.5;
    }
    break;

case 4: // horizontal and vertical split into quarters
    for (vind = 0; vind &lt; 4; ++vind) {
        view_visible[vind] = true;
        view_xport[vind] = (vind mod 2) * width * 0.5;
        view_yport[vind] = (vind div 2) * height * 0.5;
        view_wport[vind] = width * 0.5;
        view_hport[vind] = height * 0.5;
    }
    break;

default: // entire screen
    view_visible[0] = true;
    view_wport[0] = width;
    view_hport[0] = height;
}
</string>
